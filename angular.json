{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm",
    "analytics": false,
    "schematicCollections": ["@angular-eslint/schematics"]
  },
  "projects": {
    "angular-admin-dashboard": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": {
              "base": "dist/angular-admin-dashboard"
            },
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              "src/assets",
              {
                "glob": "**/*",
                "input": "dist/workers",
                "output": "assets/workers"
              }
            ],
            "styles": [
              "src/tailwind.css",
              "src/styles.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": ["./node_modules"]
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "32kB",
                  "maximumError": "64kB"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "fonts": false
              }
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "angular-admin-dashboard:build:production"
            },
            "development": {
              "buildTarget": "angular-admin-dashboard:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "build-worker": {
          "builder": "@angular-devkit/build-angular:webpack-browser",
          "options": {
            "customWebpackConfig": {
              "path": "./webpack.config.worker.js",
              "mergeRules": {
                "optimization": "append"
              }
            },
            "outputPath": "dist/workers",
            "main": "src/app/core/communication/worker/shared-worker.ts",
            "tsConfig": "tsconfig.worker.json",
            "polyfills": [],
            "assets": [],
            "styles": [],
            "scripts": [],
            "optimization": {
              "scripts": true,
              "styles": false
            },
            "outputHashing": "none",
            "sourceMap": true,
            "namedChunks": false,
            "extractLicenses": false,
            "vendorChunk": false,
            "buildOptimizer": false
          },
          "configurations": {
            "production": {
              "optimization": {
                "scripts": true,
                "styles": false,
                "fonts": false
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          }
        }
      }
    }
  }
}
